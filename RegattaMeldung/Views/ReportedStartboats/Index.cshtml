@model IEnumerable<RegattaMeldung.Models.ReportedStartboat>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Gender)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.Club)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ReportedRace)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Regatta)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Gender)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Club.ClubId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ReportedRace.CompetitionId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Regatta.RegattaId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ReportedStartboatId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ReportedStartboatId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ReportedStartboatId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
